h1>Create a new Tarot cards</h1>

{{!-- views/cards/cards-create.hbs --}}

<h2>Create a book</h2>

<form action="/cards/create" method="post">

  <label> Categories:
    <input type="text" name="categories"/>
  </label>

  <label> Star Sign:
    <input type="text" name="starsign"/>
  </label>

<label> Description:
    <input type="text" name="description"/>
  </label>

  <button type="submit"> Create </button>
</form>